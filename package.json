{
  "name": "with-typescript",
  "private": false,
  "version": "1.0.0",
  "scripts": {
    "commit": "cz",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "fix": "run-s fix:*",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "fix:prettier": "prettier \"{src,pages,test}/**/*.ts[x]\" --write",
    "fix:lint": "eslint src --ext .ts --ext .tsx --fix",
    "fix:lint-staged": "lint-staged",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "run-s build test:*",
    "test:lint": "eslint src --ext .ts --ext .tsx",
    "test:prettier": "prettier \"{src,pages,test}/**/*.ts[x]\" -l ",
    "test:spelling": "cspell \"{README.md,.github/*.md,{src,pages}/**/*.ts[x]}\"",
    "test:unit": "jest",
    "watch:test": "jest --watch",
    "pre-commit": "git-branch-is -q main && yarn fix && yarn test || echo No hooks needed on pre-commit for this branch  ",
    "pre-push": "git-branch-is -q main && npm run lint && npm run test || echo No hooks needed on pre-push for this branch ",
    "version": "standard-version"
  },
  "dependencies": {
    "@date-io/core": "1.3.13",
    "@date-io/date-fns": "1.3.13",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "4.9.1",
    "@storybook/addon-actions": "5",
    "@storybook/addon-links": "5",
    "@storybook/addon-viewport": "5",
    "@storybook/preset-typescript": "3.0.0",
    "@storybook/react": "5.X",
    "clsx": "^1.1.1",
    "faker": "5.1.0",
    "final-form": "4.20.1",
    "next": "latest",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-final-form": "6.5.2"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@testing-library/jest-dom": "5.X",
    "@testing-library/react": "11.1.0",
    "@testing-library/react-hooks": "3.4.2",
    "@types/jest": "26.X",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "4.x",
    "@typescript-eslint/parser": "4.x",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-preset-react-app": "10.0.0",
    "commitizen": "4.2.2",
    "cspell": "^4.1.0",
    "css-loader": "5.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-flow": "3.2.0",
    "eslint": "7.12.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-functional": "3.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "git-branch-is": "4.0.0",
    "husky": "4.3.0",
    "jest": "26.6.1",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "10.5.0",
    "msw": "0.21.3",
    "npm-run-all": "^4.1.5",
    "pinst": "2.0.0",
    "prettier": "2.1.2",
    "pretty-quick": "3.1.0",
    "sass": "1.29.0",
    "sass-loader": "10.0.5",
    "standard-version": "^9.0.0",
    "style-loader": "2.0.0",
    "ts-jest": "26.4.3",
    "typescript": "4.0"
  },
  "browserslist": [
    "defaults"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "printWidth": 100
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit",
      "pre-push": "npm run pre-push"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "license": "MIT"
}
